{
    "name": "roelmagdaleno/markdown-to-notion-blocks",
    "description": "Convert markdown to Notion blocks.",
    "homepage": "https://github.com/roelmagdaleno/markdown-to-notion-blocks",
    "license": "MIT",
    "keywords": [
        "notion",
        "markdown",
        "blocks"
    ],
    "autoload": {
        "psr-4": {
            "RoelMR\\MarkdownToNotionBlocks\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Roel Magdaleno",
            "email": "roelmagdaleno@gmail.com",
            "homepage": "https://roelmagdaleno.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "league/commonmark": "^2.5",
        "incenteev/emoji-pattern": "^1.3"
    },
    "require-dev": {
        "pestphp/pest": "^1.23"
    },
    "scripts": {
        "test": "vendor/bin/pest"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
